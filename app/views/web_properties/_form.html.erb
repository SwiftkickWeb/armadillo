<%= form_with(model: [@client, web_property] ) do |form| %>
  <% if web_property.errors.any? %>
    <div style="color: red">
      <h2><%= pluralize(web_property.errors.count, "error") %> prohibited this web_property from being saved:</h2>

      <ul>
        <% web_property.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div>
    <%= form.label :name, style: "display: block" %>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :url, style: "display: block" %>
    <%= form.text_field :url %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
